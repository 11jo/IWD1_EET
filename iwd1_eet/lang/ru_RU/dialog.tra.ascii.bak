@11163 = ~[COMMON WOMAN 3]~ [WOMCO03]
@11164 = ~[COMMON WOMAN 4]~ [WOMCO04]
@11165 = ~[LOW CLASS WOMAN 1]  Здорово!~ [WOMLC01]
