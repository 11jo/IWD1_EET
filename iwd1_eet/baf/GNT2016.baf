IF
	Entered([PC])
THEN
	RESPONSE #100
		DisplayString(Myself,14381)
		ReallyForceSpellRES("IDWI016",LastTrigger) //TRAP_POISON_PARALYZATION_DARTS
		Wait(2)
END

IF
	Opened([PC])
THEN
	RESPONSE #100
		DisplayString(Myself,14381) // Сработала ловушка
		ReallyForceSpellRES("IDWI016",LastTrigger) //TRAP_POISON_PARALYZATION_DARTS
		Wait(2)
END

