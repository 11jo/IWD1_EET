IF
	Global("MAP_GROUP_HOSTILE9","MYAREA",0)
	IsActive(Myself)
	See(NearestEnemyOf(Myself))
THEN
	RESPONSE #100
		SetGlobal("MAP_GROUP_HOSTILE9","MYAREA",1)
		Continue()
END

IF
	Global("MAP_GROUP_HOSTILE9","MYAREA",0)
	AttackedBy([GOODCUTOFF],DEFAULT)
THEN
	RESPONSE #100
		Enemy()
		SetGlobal("MAP_GROUP_HOSTILE9","MYAREA",1)
		Continue()
END

